{
  "name": "fhir-react-app",
  "version": "0.1.6",
  "keywords": [
    "fhir",
    "react",
    "template"
  ],
  "description": "FHIR React App Template",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "## Vite Scripts": "------------------------------------------------------",
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "staging": "vite --mode staging",
    "build-staging": "tsc && vite build --mode staging",
    "production": "vite --mode production",
    "build-production": "tsc && vite build --mode production",
    "test": "vitest",
    "preview": "vite preview",
    "## Other Scripts": "-----------------------------------------------------",
    "test:coverage": "vitest run --coverage",
    "analyze": "source-map-explorer -m 'dist/assets/*.js' --no-border-checks",
    "lint": "eslint src --ext ts,tsx",
    "prepare": "husky install",
    "format": "prettier --write src",
    "type-check": "tsc --pretty --noEmit",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "5.5.1",
    "@aws-amplify/core": "^6.4.4",
    "@aws-amplify/ui-react": "6.5.3",
    "@reduxjs/toolkit": "2.2.7",
    "antd": "5.21.0",
    "aws-amplify": "6.6.4",
    "consola": "3.2.3",
    "fhirclient": "2.5.4",
    "i18next": "23.1.0",
    "js-cookie": "3.0.5",
    "jwt-decode": "4.0.0",
    "lodash-es": "4.17.21",
    "query-string": "9.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-i18next": "13.0.0",
    "react-redux": "9.1.2",
    "react-router-dom": "6.26.2",
    "redux": "5.0.1",
    "tailwind-merge": "2.5.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@julr/vite-plugin-validate-env": "1.1.1",
    "@tailwindcss/typography": "0.5.15",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/fhir": "0.0.41",
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "husky": "9.1.6",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "source-map-explorer": "2.5.3",
    "tailwindcss": "3.4.12",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "vite": "5.4.7",
    "vite-plugin-pwa": "0.20.1",
    "vitest": "2.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  }
}
